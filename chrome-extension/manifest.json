{
  "name": "Wordle Helper",
  "version": "0.1.0.0",
  "manifest_version": 3,
  "description": "A helper extension to solve Wordle puzzles.",
  "icons": {
    "16": "icons/wordlehelper16x16.png",
    "32": "icons/wordlehelper32x32.png",
    "48": "icons/wordlehelper96x96.png",
    "128": "icons/wordlehelper256x256.png"
  },
  "action": {
    "default_icons": {
      "16": "icons/wordlehelper16x16.png",
      "32": "icons/wordlehelper32x32.png"
    },
    "default_title": "Wordle Helper",
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content-scripts/content.js"]
    }
  ]
}
